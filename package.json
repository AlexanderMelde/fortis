{
  "name": "fortis",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --configuration production --aot",
    "build-gh": "ng run fortis:build --base-href /fortis/fortis/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-silent": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ./dist/compodoc",
    "docs:build": "compodoc -p ./tsconfig.json -d ./dist/compodoc --theme material --hideGenerator",
    "docs:serve": "npm run docs:build & compodoc -s -d ./dist/compodoc",
    "storybook": "ng run fortis:storybook",
    "build-storybook": "ng run fortis:build-storybook",
    "storybook-docs": "ng run fortis:storybook-docs",
    "build-storybook-docs": "ng run fortis:build-storybook-docs",
    "generate-fake-data": "node ./server/generate-fake-data.js > ./server/database.json",
    "start-database-server": "json-server --watch ./server/database.json",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.2",
    "@angular/material-luxon-adapter": "^15.2.5",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@storybook/addon-docs": "^7.5.1",
    "faker": "^5.5.3",
    "json-server": "^0.17.3",
    "luxon": "^3.3.0",
    "ng-navigator-share": "^1.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.2",
    "@angular/cli": "~15.2.2",
    "@angular/compiler-cli": "^15.2.0",
    "@babel/core": "^7.21.0",
    "@compodoc/compodoc": "^1.1.19",
    "@cypress/schematic": "^2.5.0",
    "@storybook/addon-a11y": "^7.0.0-rc.8",
    "@storybook/addon-actions": "^7.0.0-rc.8",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.0.0-rc.8",
    "@storybook/addon-links": "^7.0.0-rc.8",
    "@storybook/angular": "^7.5.1",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.0",
    "@types/karma": "^6.3.3",
    "@types/luxon": "^3.2.1",
    "babel-loader": "^8.3.0",
    "cypress": "latest",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.1",
    "typescript": "~4.9.4"
  }
}
